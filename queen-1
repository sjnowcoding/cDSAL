#include <stdio.h>
#include <windows.h>
int pos[8];

void print() {
	for(int i=0; i<8; i++)
		printf("%2d", pos[i]);
	printf("\n");
}

void setQueen(int col) {
	
	for(int j=0; j<8; j++) {
		pos[col]=j;
		if(col==7){ print(); Sleep(1000);   }
		else setQueen(col+1);
	}
}

int main() {
	setQueen(0);
	return 0;
}


	
